@page
@model Projet_AdoNet.Pages.Commerciaux.DetailsModel

@using Microsoft.AspNetCore.Http

@{
    ViewData["Title"] = "Details du Commercial";
}

@*Gestion de la bouton de déconnexion*@
<script src="~/js/ButtonControl.js"></script>

@*Test de redirection*@
@if (HttpContext.Session.GetString("login") == null)
{
    <script src="~/js/Redirect.js"></script>
}


<br />

<div class="text-center">
    <h1 class="display-4">@ViewData["Title"]</h1>
</div>

<br />
<br />

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Commercial.Nom)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Commercial.Nom)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Commercial.Prenom)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Commercial.Prenom)
        </dd>
    </dl>
</div>
